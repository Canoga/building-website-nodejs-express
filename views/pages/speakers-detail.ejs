<div class="container mt-4">
  <div class="row">
    <div class="col-md-8">
      <article class="speakerslist maincontent">
        <h1 class="speakerslist-title"><%=totalSpeaker.speaker.title%></h1>
        <div class="speakerslist-name">
          with
          <a href="/speakers/<%=totalSpeaker.speaker.shortname%>"><%=totalSpeaker.speaker.name%></a>
        </div>
        <p class="speakerslist-info mt-2">
          <a href="/speakers/<%=totalSpeaker.speaker.shortname%>"> </a>
          <img
            class="speakerslist-img img-fluid"
            src="/images/speakers/<%=totalSpeaker.speaker.shortname%>.jpg"
            alt="Photo of <%=totalSpeaker.speaker.name%>"
          />
        </p>
        <%-totalSpeaker.speaker.description%>
        <p></p>
      </article>
    </div>
    <aside class="col-md-4">
      
      <%- include('./partials/artworkSidebar') %>
    </aside>
  </div>
</div>
